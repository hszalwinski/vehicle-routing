simulation-scan-all:
	for number in 4 5 6 7 8 9 10 ; do \
        pipenv run python vrp.py simulation \
		-al scan-all \
		-i 100 \
		-d data/distance_matrix/e$${number}.pickle \
		-c data/configurations/conf1.json \
		-v data/vehicles/vehicles1.json \
		-o data/results/scan-all.csv ; \
    done
	pipenv run python vrp.py simulation \
		-al scan-all \
		-i 50 \
		-d data/distance_matrix/e11.pickle \
		-c data/configurations/conf1.json \
		-v data/vehicles/vehicles1.json \
		-o data/results/scan-all.csv
	pipenv run python vrp.py simulation \
		-al scan-all \
		-i 10 \
		-d data/distance_matrix/e12.pickle \
		-c data/configurations/conf1.json \
		-v data/vehicles/vehicles1.json \
		-o data/results/scan-all.csv
	pipenv run python vrp.py simulation \
		-al scan-all \
		-i 1 \
		-d data/distance_matrix/e13.pickle \
		-c data/configurations/conf1.json \
		-v data/vehicles/vehicles1.json \
		-o data/results/scan-all.csv

